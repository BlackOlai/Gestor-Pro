{
  "name": "consultia-api",
  "version": "1.0.0",
  "description": "Backend API for ConsultIA platform",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && cp -r src/types dist/ || echo 'Types copy failed'",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts",
    "prestart": "npm run build",
    "prepare": "npm run build",
    "vercel-build": "npm install && tsc && cp -r src/types dist/ || echo 'Types copy failed'"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.13",
    "@vercel/node": "^5.3.21",
    "nodemon": "^3.0.3",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
